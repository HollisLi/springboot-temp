<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.temp.biz.mapper.system.SystemUserMapper">

    <select id="getByUsername" resultType="com.temp.biz.entity.system.SystemUser">
        SELECT *
        FROM system_user
        WHERE username = #{username}
          and delete_flag = 0
    </select>
</mapper>